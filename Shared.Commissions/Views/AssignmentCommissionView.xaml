<UserControl x:Class="Shared.Commissions.Views.AssignmentCommissionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:fluent="urn:fluent-ribbon"
             xmlns:viewModels="clr-namespace:Shared.Commissions.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        
            <fluent:Button Size="Large"
                       LargeIcon="pack://application:,,,/Core;Component/Resources/Images/DocumentImport48x48.png"
                       ToolTip="Направление на комиссию"
                       Header="Направление на комиссию"
                       Command="{Binding AssignCommand}"/>
            
    </Grid>
</UserControl>
